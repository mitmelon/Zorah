{
  "name": "mitmelon/vars",
  "description": "Advanced REST API email and sms security system that protects emails and sms inbox from spam and cyber threats. Perform email &amp; sms grouping, notifications and other interesting features.",
  "license": "MIT",
  "authors": [
    {
      "email": "manomitehq@gmail.com",
      "name": "Manomite"
    }
  ],
  "autoload": {
    "psr-4": {
      "Manomite\\": [
        "src/",
        "kernel/"
      ]
